      </geometry>
    </visual>
  </link>

<!-- camera-movable-fixation2 -->
  <link name="camera-movable-fixation2">
    <pose>1.04 0.62 2.25 0 0 0</pose>			<!-- to move the camera edit the y-coordinate -->
    <inertial>
      <mass>1.84</mass>
      <inertia><ixx>0.294</ixx><iyy>0.294</iyy><izz>0.023</izz><ixy>0</ixy><ixz>0</ixz><iyz>0</iyz></inertia>
    </inertial>
    <collision name="collision">
      <geometry>
          <mesh>
            <uri>model://workcell-assembly-v2/meshes/camera-movable-fixation.stl</uri>
