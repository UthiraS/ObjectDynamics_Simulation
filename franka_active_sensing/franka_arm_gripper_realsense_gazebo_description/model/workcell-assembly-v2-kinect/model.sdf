<?xml version='1.0'?>
<sdf version="1.4">
<model name="workcell-assembly-v2">
<static>false</static>
  
  <!-- workcell-assembly-v2 -->
  <link name="workcell-assembly-v2">
    <pose>0 0 0.07 0 0 0</pose>
    <inertial>
      <mass>13.55</mass>
      <inertia><ixx>0.294</ixx><iyy>0.294</iyy><izz>0.023</izz><ixy>0</ixy><ixz>0</ixz><iyz>0</iyz></inertia>
    </inertial>
